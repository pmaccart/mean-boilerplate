.navbar.navbar-static-top(ng-controller="HeaderCtrl")
  .navbar-links(role="navigation")
    .container
      .navbar-left
        ul.nav.navbar.nav
          li
            a(href="http://www.slalom.com") Slalom.com
      .navbar-right
        ul.nav.navbar-nav
          li
            a(href="http://www.google.com") Google
          li
            a(href="http://www.twitter.com") Twitter
          li
            a(href="http://www.cnn.com") CNN

  .navbar-inverse(role="navigation")
    .container
      .navbar-header
        button.navbar-toggle(type="button", data-toggle="collapse", data-target=".navbar-collapse")
          span.sr-only Toggle Nagivation
          span.icon-bar
          span.icon-bar
          span.icon-bar
        a.navbar-brand(href="#") Sample App
      .collapse.navbar-collapse
        ul.nav.navbar-nav
          li.active
            a(href="#") Home
          li
            a(href="#about") About
          li
            a(href="#contact") Contact

        ul.nav.navbar-nav.navbar-right
          li.dropdown
            a.dropdown-toggle Hello, {{user.username}}
              b.caret
            ul.dropdown-menu
              li
                a(href="#") My Account
              li
                a(href="#") Logout




